{
  "name": "sciquest-heroes",
  "version": "1.0.0",
  "description": "AI-powered storytelling platform that turns science into personalized voice-narrated comics for kids",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:setup": "node tests/setup-test-data.js",
    "test:cleanup": "node tests/cleanup-test-data.js",
    "test:enrollment": "playwright test tests/parent-enrollment-comprehensive.spec.js && node tests/generate-enrollment-report.js"
  },
  "keywords": [
    "education",
    "science",
    "kids",
    "storytelling",
    "ai"
  ],
  "author": "SciQuest Heroes",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv": "^16.3.1",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
